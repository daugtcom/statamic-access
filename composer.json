{
    "name": "daugtcom/statamic-access",
    "autoload": {
        "psr-4": {
            "Daugt\\Access\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Daugt\\Access\\Tests\\": "tests"
        }
    },
    "require": {
        "statamic/cms": "^6.0"
    },
    "require-dev": {
        "orchestra/testbench": "^9.2 || ^10.0"
    },
    "config": {
        "allow-plugins": {
            "pixelfear/composer-dist-plugin": true
        }
    },
    "extra": {
        "statamic": {
            "name": "Access",
            "description": "Access addon"
        },
        "laravel": {
            "providers": [
                "Daugt\\Access\\ServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
